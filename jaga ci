pipeline {
    options([buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '2', numToKeepStr: '3'))])
    agent any
    stages{
        stage("clone"){
            steps{
            checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/efsavage/hello-world-war.git']]])
            }
        }
        stage("build"){
            steps{
            sh 'mvn package'
            }
        }
    }  
}
